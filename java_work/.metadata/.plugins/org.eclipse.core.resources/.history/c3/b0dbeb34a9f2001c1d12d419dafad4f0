package jjj;



public class example {
	
	
	public static void main(String[] args) {
		final double KM_PER_MILE = 1.609344;
		double km;
		double mile = 60.0;
		
		km = mile * KM_PER_MILE;
		
		System.out.println(mile+ "마일은 " + km + "킬로미터 입니다.");
	}
}
